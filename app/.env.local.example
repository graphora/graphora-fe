# Graphora FE development defaults

# Auth Bypass - Set to 'true' for local development without Clerk
# When enabled, skips Clerk authentication entirely
NEXT_PUBLIC_AUTH_BYPASS=false
AUTH_BYPASS_USER_ID=local-dev-user
AUTH_BYPASS_EMAIL=dev@localhost

# Clerk Authentication (required when NEXT_PUBLIC_AUTH_BYPASS is not 'true')
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up

BACKEND_API_URL=http://localhost:8000
NEXT_PUBLIC_BACKEND_API_URL=http://localhost:8000

# Prefect dashboards (optional)
NEXT_PUBLIC_TRANSFORM_PREFECT_STATUS_URL=http://localhost:4200/ui/flows
NEXT_PUBLIC_MERGE_PREFECT_STATUS_URL=http://localhost:4200/ui/flows
